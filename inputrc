"\e[1~": beginning-of-line
"\e[4~": end-of-line
"\e[5~": beginning-of-history
"\e[6~": end-of-history
"\e[3~": delete-char
"\e[2~": quoted-insert
#"\e[C": forward-word
#"\e[D": backward-word
"\ep": history-search-backward
"\es": history-search-forward

set completion-ignore-case On
set meta-flag on
set input-meta on
set output-meta on
set convert-meta off


