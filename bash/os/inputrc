"\e[1~": beginning-of-line
"\e[4~": end-of-line
"\e[5~": beginning-of-history
"\e[6~": end-of-history
"\e[3~": delete-char
"\e[2~": quoted-insert
"\ep": history-search-backward
"\es": history-search-forward
"\e[1;9D": backward-word
"\e[1;9C": forward-word

set completion-ignore-case On
set meta-flag on
set input-meta on
set output-meta on
set convert-meta off


